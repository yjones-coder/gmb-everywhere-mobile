n{
  "name": "app-template",
  "version": "1.0.0",
  "type": "module",
  "private": true,
  "main": "expo-router/entry",
  "scripts": {
    "dev": "concurrently -k \"npm run dev:server\" \"npm run dev:web\"",
    "dev:server": "cross-env NODE_ENV=development tsx watch server/_core/index.ts",
    "dev:web": "cross-env EXPO_USE_METRO_WORKSPACE_ROOT=1 npx expo start --web --port 8081",
    "build": "esbuild server/_core/index.ts --platform=node --packages=external --bundle --format=esm --outdir=dist",
    "start": "NODE_ENV=production node dist/index.js",
    "check": "tsc --noEmit",
    "lint": "eslint",
    "format": "prettier --write .",
    "test": "vitest run",
    "db:push": "drizzle-kit generate && drizzle-kit migrate"
  },
  "dependencies": {
    "@expo/vector-icons": "^15.0.3",
    "@react-google-maps/api": "^2.20.8",
    "@tanstack/react-query": "^5.60.0",
    "@trpc/client": "11.7.2",
    "@trpc/react-query": "11.7.2",
    "@trpc/server": "11.7.2",
    "axios": "^1.12.0",
    "cookie": "^1.0.2",
    "dotenv": "^16.4.7",
    "drizzle-orm": "^0.44.5",
    "expo-router": "~6.0.19",
    "express": "^4.21.2",
    "jose": "6.1.0",
    "mysql2": "^3.15.0",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native-svg": "15.12.1",
    "superjson": "^1.13.3",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@types/cookie": "^0.6.0",
    "@types/express": "^4.17.21",
    "@types/node": "^22.8.1",
    "@types/react": "~19.1.0",
    "concurrently": "^9.1.0",
    "cross-env": "^7.0.3",
    "drizzle-kit": "^0.31.4",
    "esbuild": "^0.25.0",
    "eslint": "^9.25.0",
    "eslint-config-expo": "~10.0.0",
    "prettier": "^3.6.2",
    "tsx": "^4.19.1",
    "typescript": "~5.9.2",
    "vitest": "^2.1.4"
  },
  "packageManager": "pnpm@9.12.0"
}